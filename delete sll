import java.util.*;
class linkedlist{
    Node head;
    class Node{
        int data;
        Node next;
        Node(int val){
            data = val;
            next = null;
        }
    }
    linkedlist(){
        head = null;
    }
    public void insertend(int val){
        Node newnode = new Node(val);
        if (head==null){
            head = newnode;
        }
        else{
            Node temp = head;
            while(temp.next != null){
                temp =  temp.next;
            }
            temp.next = newnode;
        }
    }
    void deletebegin() {
        head=head.next;
    }
    void deletepos(int pos)
    {
        Node temp=head;
        Node prev=null;
        for(int i=0;i<pos;i++)
        {
            prev=temp;
            temp=temp.next;
        }
        prev.next=temp.next;
    }
    void deleteend(int m)
    {
        Node temp=head;
        Node prev=null;
        for(int i=1;i<m;i++) {
            prev= temp;
            temp = temp.next;
        }
        prev.next=null;
    }

    public void display(){
        Node temp = head;
        while(temp != null){
            System.out.print(temp.data+"-->");
            temp = temp.next;
        }
        System.out.print("Null");
    }
    public static void main(String args[]){
        Scanner sc = new Scanner (System.in);
        linkedlist list = new linkedlist();
        int m = sc.nextInt();
        for  (int i =0 ;i<m;i++){
            int val = sc.nextInt();
            list.insertend(val);}
       list.deletebegin();
        int position=sc.nextInt();
       list.deletepos(position);
      // list.deleteend(m);
        list.display();
    }
}
