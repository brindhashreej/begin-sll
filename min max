import java.util.*;
class linkedlist{
    Node head;
    class Node{
        int data;
        Node next;
        Node(int val){
            data = val;
            next = null;
        }
    }
    linkedlist(){
        head = null;
    }
    public void insertend(int val){
        Node newnode = new Node(val);
        if (head==null){
            head = newnode;
        }
        else{
            Node temp = head;
            while(temp.next != null){
                temp =  temp.next;
            }
            temp.next = newnode;
        }
    }
    public void display(){
        Node temp = head;
        while(temp != null){
            System.out.print(temp.data+"-->");
            temp = temp.next;
        }
       System.out.println("Null");
    }
    public void maxi() {
        Node current = head;
        int maxi;
        if (head == null) {
            System.out.print("empty");
        } else {
            maxi = head.data;
            while (current != null) {
                if (maxi < current.data) {
                    maxi = current.data;
                }
                current = current.next;
            }
                System.out.println(maxi);
        }
    }
    public void min() {
        Node current = head;
        int min;
        if (head == null) {
            System.out.print("empty");
        } else {
            min = head.data;
            while (current != null) {
                if (min > current.data) {
                    min = current.data;
                }
                current = current.next;
            }
            System.out.print(min);
        }
    }
    public static void main(String args[]){
        Scanner sc = new Scanner (System.in);
        linkedlist list = new linkedlist();
        int m = sc.nextInt();
        for (int i =0 ;i<m;i++) {
            int val = sc.nextInt();
            list.insertend(val);
        }
        list.display();
        list.maxi();
        list.min();
        }
}
